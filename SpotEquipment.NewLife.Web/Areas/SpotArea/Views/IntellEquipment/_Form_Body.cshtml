@model NewLife.Cube.Entity.IntelligentTerminal
@using NewLife;
@using XCode;
@using XCode.Configuration;
@{
    var entity = Model;
    var fields = ViewBag.Fields as IList<FieldItem>;
    var isNew = (entity as IEntity).IsNullKey;
    var fid = entity.Number1;
}
<script>
    @*$(function () {
        $("#sa").click(function () {
        {
            window.location.href = "/FlowEquipment/GetSpotEdit?fid=@fid";
        }})
        //return confirm("Are you OK?");
    })*@
    function cc() {
       // document.getElementById("sa").click();
         window.location.href = "/FlowEquipment/GetSpotEdit?fid=@fid";
    }
</script>
<div class="form-group col-xs-12 col-sm-6 col-lg-4">
    <label class="control-label col-xs-3 col-sm-3">设备编号</label>
    <div class="input-group col-xs-9 col-sm-9">
        @Html.TextBox("Number1", @entity.Number1, new { @class = "form-control", role="number" })
    </div>
</div>
<div class="form-group col-xs-12 col-sm-6 col-lg-4">
    <label class="control-label col-xs-3 col-sm-3">现场设备编号</label>
    <div class="input-group col-xs-9 col-sm-9">
        <input class="form-control" id="SpotEquipmentNumber" name="SpotEquipmentNumber" type="text" value="@entity.SpotEquipmentNumber" />
    </div>
</div>
<div class="form-group col-xs-12 col-sm-6 col-lg-4">
    <label class="control-label col-xs-3 col-sm-3">通讯方式</label>
    <div class="input-group col-xs-9 col-sm-9">
        @Html.ForEnum("ConnMode", @entity.ConnMode)
    </div>
</div>
<div class="form-group col-xs-12 col-sm-6 col-lg-4">
    <label class="control-label col-xs-3 col-sm-3">电话号码</label>
    <div class="input-group col-xs-9 col-sm-9">
        <input class="form-control" id="TelephoneNumber" name="TelephoneNumber" type="text" value="@entity.TelephoneNumber" />
    </div>
</div>
<div class="form-group col-xs-12 col-sm-6 col-lg-4">
    <label class="control-label col-xs-3 col-sm-3">费用套餐</label>
    <div class="input-group col-xs-9 col-sm-9">
        <input class="form-control" id="CostPackage" name="CostPackage" type="text" value="@entity.CostPackage" />
    </div>
</div>
<div class="form-group col-xs-12 col-sm-6 col-lg-4">
    <label class="control-label col-xs-3 col-sm-3">续费周期</label>
    <div class="input-group col-xs-9 col-sm-9">
        <input class="form-control" id="RenewalCycle" name="RenewalCycle" type="text" value="@entity.RenewalCycle" />
    </div>
</div>
<div class="form-group col-xs-12 col-sm-6 col-lg-4">
    <label class="control-label col-xs-3 col-sm-3">本次续费时间</label>
    <div class="input-group col-xs-9 col-sm-9">
        <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
        @Html.TextBox("ThisRenewalTime", @entity.ThisRenewalTime.ToFullString(""), new { @class = "form-control date form_datetime" })
    </div>
</div>
@Html.Partial("_Form_Footer", entity)
@if (this.Has(PermissionFlags.Insert, PermissionFlags.Update))
{
<div class="clearfix form-actions col-sm-12 col-md-12">
    <label class="control-label col-xs-4 col-sm-5 col-md-5"></label>
    <button type="submit" class="btn btn-success btn-sm"><i class="glyphicon glyphicon-@(isNew ? "plus" : "save")"></i><strong>@(isNew ? "新增" : "保存")</strong></button>
    <button type="button" class="btn btn-danger btn-sm" onclick="history.go(-1);"><i class="glyphicon glyphicon-remove"></i><strong>取消</strong></button>
    @if (!isNew)
    {
        <button type="button" class="btn btn-success btn-sm" onclick="cc()"><strong>编辑公共属性</strong></button>
        @*<a href="/FlowEquipment/GetSpotEdit?fid=@entity.Number1" class="easyui-linkbutton" plain="true" iconCls="icon-power-blue" onclick="cc(this)">编辑公共属性</a>*@
    }
</div>
}